  "bugs": {
    "url": "https://github.com/northcoders/be-nc-news/issues"
  },
  "homepage": "https://github.com/northcoders/be-nc-news#readme",
  "jest": {
    "setupFilesAfterEnv": [
      "jest-sorted"
    ]
  },
  "dependencies": {
    "express": "^4.17.1",
    "knex": "^0.21.12",
    "pg": "^8.4.2"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "jest-sorted": "^1.0.8",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.1"
